{
    "name": "qsnh/meedu",
    "description": "A video system about edu.",
    "keywords": [
        "meedu"
    ],
    "homepage": "https://meedu.vip",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "alibabacloud/client": "^1.5",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "jacobcyl/ali-oss-storage": "^2.1",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.2",
        "laravel/socialite": "^4.0",
        "league/html-to-markdown": "^4.8",
        "mews/captcha": "^3.0",
        "mews/purifier": "^3.0",
        "mobiledetect/mobiledetectlib": "^2.8",
        "nelexa/zip": "^3.3",
        "overtrue/easy-sms": "^1.1",
        "overtrue/pinyin": "~3.0",
        "overtrue/wechat": "~4.0",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "socialiteproviders/qq": "^3.0",
        "socialiteproviders/weixin-web": "dev-master",
        "tencentcloud/tencentcloud-sdk-php": "^3.0",
        "tymon/jwt-auth": "^1.0",
        "yansongda/laravel-pay": "^2.0",
        "zgldh/qiniu-laravel-storage": "^0.10.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "darkaonline/l5-swagger": "^6.0",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "5.1.3",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^8.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Addons\\": "addons/"
        },
        "files": [
            "./app/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": "./vendor/bin/phpunit",
        "cf": "php-cs-fixer fix",
        "clean": [
            "@php artisan route:clear",
            "@php artisan config:clear",
            "@php artisan view:clear"
        ],
        "rebuild": [
            "@php artisan route:clear",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "@php artisan route:cache",
            "@php artisan config:cache"
        ],
        "restart-sp": [
            "supervisorctl reread",
            "supervisorctl update",
            "supervisorctl start meedu:*"
        ],
        "apidoc": "@php artisan l5-swagger:generate"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
