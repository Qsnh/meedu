{
    "name": "qsnh/meedu",
    "description": "meedu",
    "keywords": [
        "meedu"
    ],
    "homepage": "https://meedu.vip",
    "type": "project",
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "alibabacloud/client": "^1.5",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "intervention/image": "^2.7",
        "jacobcyl/ali-oss-storage": "^2.1",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.2",
        "laravel/scout": "^9.2",
        "laravel/socialite": "^5.2.0",
        "meilisearch/meilisearch-php": "^0.19.0",
        "mews/captcha": "^3.0",
        "mews/purifier": "^3.0",
        "mobiledetect/mobiledetectlib": "^2.8",
        "nelexa/zip": "^3.3",
        "overtrue/laravel-filesystem-cos": "1.0.2",
        "overtrue/wechat": "~4.0",
        "pion/laravel-chunk-upload": "^1.5",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "socialiteproviders/qq": "^4.0",
        "tencentcloud/faceid": "3.0.696",
        "tencentcloud/live": "3.0.696",
        "tencentcloud/sms": "3.0.696",
        "tencentcloud/vod": "3.0.696",
        "tymon/jwt-auth": "^1.0",
        "yansongda/laravel-pay": "^2.0",
        "zgldh/qiniu-laravel-storage": "^0.10.3",
        "zoujingli/ip2region": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^6.2.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Addons\\": "addons/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "./app/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": "./vendor/bin/phpunit",
        "clean": [
            "@php artisan route:clear",
            "@php artisan config:clear",
            "@php artisan view:clear"
        ],
        "rebuild": [
            "@php artisan route:clear",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "@php artisan route:cache",
            "@php artisan config:cache",
            "@php artisan view:cache"
        ],
        "restart-sp": [
            "supervisorctl reread",
            "supervisorctl update",
            "supervisorctl start meedu:*"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "symfony/thanks": true,
            "easywechat-composer/easywechat-composer": true,
            "php-http/discovery": true
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
